	string str;
	char a;
	int x;
	while (file >> str)
	{
		int len = str.length();
		for(int i =0; i < len;i++)
		{
			a = str[i];
			if ((a - '0')<11 and (a - '0') >= 0)
			{
				x = a - '0';
				cout << x << " ";
			}
		}
	}
}




#include <iostream>
#include <cmath>
using namespace std;



int main() {
	string s = "AAAAAaaaaazzzZZZZZZbbZZZZZZZzzzbbbbbbb";
	int j = 0;
	for (int i = 0; i < s.length(); i++)
	{
		for (j = 0; j < s.length() - i - 1; j++)
		{
			float ind1;
			float ind2;
			if (s[j] > 96)
			{
				ind1 = (s[j]%1000) - 31.5;
			}
			else
			{
				ind1 = s[j] % 1000;
			}
			if (s[j+1] > 96)
			{
				ind2 = (s[j+1]%1000) - 31.5;
			}
			else
			{
				ind2 = s[j + 1] % 1000;
			}
			if (ind1 > ind2)
			{
				int tmp = s[j];
				s[j] = s[j + 1];
				s[j + 1] = tmp;
			}
		}
	}
	cout << s << endl;
	return 0;
}



n = int(input())
lis = []
for _ in range(n):
    sps = [float(i) for i in input().split()]
    lis.append(sps)
for x in range(n):
    print(lis[x])

for i in range(0,n):
    for j in range(i+1,n):
        if lis[j][i] != 0:
            k = -lis[i][i]/lis[j][i]
            for x in range(len(lis[j])):
                lis[j][x] = lis[j][x] * k + lis[i][x]

for x in range(n-1,-1,-1):
    if lis[x][x] != 1:
        k = lis[x][x]
        for z in range(len(lis[x])):
            lis[x][z] = lis[x][z] / k
    break
print("")

for x in range(n-1,0,-1):
    for z in range(x-1,-1,-1):
        k = -lis[z][x]/lis[x][x]

        for i in range(len(lis[z])):
            lis[z][i] = lis[z][i] + lis[x][i] * k

for x in range(n):
    if lis[x][x] != 1:
        k = lis[x][x]
        for z in range(len(lis[x])):
            lis[x][z] = lis[x][z] / k

print('viv')
for x in range(n):
    print(lis[x])


